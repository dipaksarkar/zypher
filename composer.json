{
    "name": "zypher/encoder",
    "description": "PHP source code encryption and licensing system using AES-256-CBC encryption",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Zypher Team"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1"
    },
    "autoload": {
        "psr-4": {
            "Zypher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zypher\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/zypher"
    ],
    "scripts": {
        "encode": "php bin/zypher",
        "test": "phpunit"
    }
}
